a = int(input("Enter first number: "))
b = int(input("Enter second number: "))

print("Addition:", a + b)
print("Subtraction:", a - b)
print("Multiplication:", a * b)
print("Division:", a / b)

print("a > b and a != 0:", a > b and a != 0)
print("a < b or b != 0:", a < b or b != 0)
print("not(a > b):", not(a > b))
